<swiper indicator-dots="true" autoplay="true" interval="5000" duration="1000" style="height:{{sHeight}};">    
  <block wx:for="{{imgList}}" wx:key="key">    
    <swiper-item>    
      <image src="{{item}}" class="slide-image" mode="aspectFill"/>    
    </swiper-item>    
  </block>    
</swiper> 